{
  "name": "bottender-docs",
  "description": "The Bottender docs",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && cp CNAME public/ && gh-pages -d public -b master -m 'Auto-generated commit [ci skip]'",
    "dev": "gatsby develop",
    "dev:mobile": "gatsby develop -H 0.0.0.0",
    "develop": "npm run dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:staged": "lint-staged",
    "precommit": "npm run lint:css && lint-staged",
    "reset": "rimraf ./.cache",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^1.9.238",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-glamor": "^1.6.13",
    "gatsby-plugin-google-analytics": "^1.0.24",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sitemap": "^1.2.18",
    "gatsby-plugin-styled-components": "^2.0.9",
    "gatsby-plugin-typography": "^1.7.18",
    "gatsby-remark-autolink-headers": "^1.4.13",
    "gatsby-remark-images": "^1.5.56",
    "gatsby-remark-prismjs": "^1.2.20",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "glamorous": "^4.12.1",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-burger-menu": "^2.3.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.2.3",
    "styled-normalize": "^4.0.0",
    "typography": "^0.16.6",
    "typography-theme-kirkham": "^0.16.3"
  },
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-yoctol": "^0.16.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.1.3",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^0.7.0",
    "stylelint-processor-styled-components": "^1.3.1"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
