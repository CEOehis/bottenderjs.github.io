{
  "private": true,
  "name": "bottender-docs",
  "description": "The Bottender docs",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "precommit": "lint-staged",
    "deploy":
      "gatsby build && cp CNAME public/ && gh-pages -d public -b master -m 'Auto-generated commit [ci skip]'",
    "dev": "gatsby develop",
    "dev:mobile": "gatsby develop -H 0.0.0.0",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.js'",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint src",
    "lint:staged": "lint-staged",
    "reset": "rimraf ./.cache",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^1.9.261",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-glamor": "^1.6.13",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-plugin-typography": "^1.7.18",
    "gatsby-remark-autolink-headers": "^1.4.18",
    "gatsby-remark-images": "^1.5.63",
    "gatsby-remark-prismjs": "^1.2.20",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "glamorous": "^4.13.0",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-burger-menu": "^2.4.4",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "typography": "^0.16.17",
    "typography-theme-kirkham": "^0.16.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-yoctol": "^0.16.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.6.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-config-yoctol": "^0.7.0",
    "stylelint-processor-styled-components": "^1.3.1"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "stylelint", "git add"],
    "*.{json,md}": ["prettier --write", "git add"],
    ".babelrc": ["prettier --write", "git add"],
    "package.json": [
      "prettier-package-json --write",
      "prettier --write",
      "git add"
    ]
  }
}
